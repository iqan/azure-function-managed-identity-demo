{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request;Exception"
            }
        },
        "logLevel": {
            "default": "Information"
        }
    },
    "extensions": {
        "serviceBus": {
            "messageHandlerOptions": {
                "autoComplete": true
            }
        }
    }
}